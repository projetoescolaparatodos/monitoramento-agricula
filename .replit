args = "npm run build"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server/index.ts"

[[workflows.workflow]]
name = "Run on Different Port"
author = 40255565
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "PORT=5001 npm run dev"
