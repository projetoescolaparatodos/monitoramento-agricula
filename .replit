[[workflows.workflow]]
name = "Rasa Production"
author = 40255565
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "rasa train"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "rasa run --enable-api --cors \"*\" --port 5005 --log-file rasa.log"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "rasa run actions --port 5055 --log-file actions.log"

[[workflows.workflow]]
name = "Rasa Optimized"
author = 40255565
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "rasa train --augmentation 0"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "rasa run --enable-api --cors \"*\" --port 5005 --endpoints endpoints.yml"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "rasa run actions --port 5055"

[[workflows.workflow]]
name = "Run"
author = 40255565
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[workflows.workflow]]
name = "Build e Executar Produção"
author = 40255565
mode = "sequential"

[[workflows.workflow.tasks]]
